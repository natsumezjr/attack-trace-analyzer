[run]
source = app
omit =
    */tests/*
    */test_*.py
    */__pycache__/*
branch = true

[report]
precision = 2
show_missing = true
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml
